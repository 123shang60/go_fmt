	b4 := strings.Count(s, "a") != 0 // strings.Count(s, "a") != 0
	b2 := strings.Contains(s, "a")
	b5 := !strings.Contains(s, "a")
	log.Println(b1, b2, b3, b4, b5)

	//	if strings.Count(s, "a") == 0 || true {
